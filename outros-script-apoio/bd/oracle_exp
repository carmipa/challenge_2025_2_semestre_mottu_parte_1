--  DDL for Procedure AUMENTA_SALARIO
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "RM557881"."AUMENTA_SALARIO" 
(
    P_codigo IN NUMBER
,    P_PERCENT IN NUMBER
)AS
BEGIN
    UPDATE emp SET sal = sal + sal * (p_percent/100);
END AUMENTA_SALARIO;

-- 

CREATE OR REPLACE PROCEDURE AUMENTA_SALARIO
(
    P_codigo IN NUMBER
,    P_PERCENT IN NUMBER
)AS
BEGIN
    UPDATE emp SET sal = sal + sal * (p_percent/100);
END AUMENTA_SALARIO;

SELECT SAL FROM EMP;

EXEC AUMENTA_SALARIO(130,10);

ROLLBACK;

--

CREATE OR REPLACE PROCEDURE AUMENTA_SALARIO
(
    P_codigo IN NUMBER
,    P_PERCENT IN NUMBER
)AS
BEGIN
    UPDATE emp SET sal = sal + sal * (p_percent/100)
    WHERE empno = p_codigo;
END AUMENTA_SALARIO;

/
