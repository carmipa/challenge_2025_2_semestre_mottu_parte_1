--  DDL for Trigger AGENDAR_ID_AGE_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "RM557881"."AGENDAR_ID_AGE_TRG" 
    before insert
    on AGENDAR
    for each row
     WHEN (new.id_age IS NULL) BEGIN
    :new.id_age := agendar_id_age_seq.nextval;
END;
